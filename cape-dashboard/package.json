{
  "name": "cape-dashboard",
  "version": "2.0.0",
  "license": "AGPLv3",
  "scripts": {
    "ng": "ng",
    "conventional-changelog": "conventional-changelog",
    "start": "ng serve --port 80",
    "build": "ng build",
    "build:prod": "npm run build -- --prod --aot",
    "test": "ng test",
    "test:coverage": "rimraf coverage && npm run test -- --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint cape-dashboard --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "format": "prettier --write \"**/*.{js,json,jsx,ts,tsx}\"",
    "format-check": "prettier --check \"**/*.{js,json,jsx,ts,tsx}\"",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "docs": "compodoc -p src/tsconfig.app.json -d docs",
    "docs:serve": "compodoc -p src/tsconfig.app.json -d docs -s",
    "prepush": "npm run lint:ci",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "postinstall": "ngcc --properties es2015 es5 browser module main --first-only --create-ivy-entry-points --tsconfig \"./src/tsconfig.app.json\""
  },
  "dependencies": {
    "@akveo/ng2-completer": "^9.0.1",
    "@angular/animations": "11.2.12",
    "@angular/cdk": "11.2.11",
    "@angular/common": "11.2.12",
    "@angular/compiler": "11.2.12",
    "@angular/core": "11.2.12",
    "@angular/forms": "11.2.12",
    "@angular/localize": "11.2.12",
    "@angular/material": "11.2.11",
    "@angular/platform-browser": "11.2.12",
    "@angular/platform-browser-dynamic": "11.2.12",
    "@angular/platform-server": "11.2.12",
    "@angular/router": "11.2.12",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@nebular/auth": "7.0.0",
    "@nebular/date-fns": "7.0.0",
    "@nebular/eva-icons": "7.0.0",
    "@nebular/security": "7.0.0",
    "@nebular/theme": "7.0.0",
    "@ng-bootstrap/ng-bootstrap": "9.1.0",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@swimlane/ngx-charts": "17.0.1",
    "bootstrap": "4.6.0",
    "classlist.js": "1.1.20150312",
    "d3": "6.6.2",
    "d3-mitch-tree": "1.1.2",
    "date-fns": "2.20.1",
    "eva-icons": "1.1.3",
    "file-saver": "2.0.5",
    "intl": "1.2.5",
    "ionicons": "2.0.1",
    "material-design-icons": "3.0.1",
    "nebular-icons": "1.1.0",
    "ng2-smart-table": "1.7.2",
    "ngx-configure": "9.0.0",
    "node-sass": "5.0.0",
    "normalize.css": "8.0.1",
    "pace-js": "1.2.4",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.6.7",
    "rxjs-compat": "6.6.7",
    "socicon": "3.0.5",
    "tslib": "2.2.0",
    "typeface-exo": "1.1.13",
    "uuid": "^8.3.1",
    "web-animations-js": "^2.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.11",
    "@angular-eslint/builder": "4.2.0",
    "@angular-eslint/eslint-plugin": "4.2.0",
    "@angular-eslint/eslint-plugin-template": "4.2.0",
    "@angular-eslint/schematics": "4.2.0",
    "@angular-eslint/template-parser": "4.2.0",
    "@angular/cli": "11.2.11",
    "@angular/compiler-cli": "11.2.12",
    "@angular/language-service": "11.2.12",
    "@compodoc/compodoc": "1.1.11",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@types/d3-color": "2.0.1",
    "@types/jasmine": "3.6.9",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.14.37",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "conventional-changelog-cli": "2.1.1",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "33.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "6.0.0",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "lint-staged": "10.5.4",
    "ng2-completer": "9.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "3.0.2",
    "stylelint": "13.12.0",
    "ts-node": "9.1.1",
    "typescript": "~4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ],
    "*.{css,md}": [
      "prettier . --write",
      "git add"
    ]
  }
}
